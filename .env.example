PRIVATE_KEY=
PUBLIC_ADDRESS=
TYCHO_API_KEY=sampletoken
TYCHO_URL=tycho-unichain-beta.propellerheads.xyz
UNICHAIN_RPC_URL=https://unichain-rpc.publicnode.com
ARBITRUM_RPC_URL=https://arb1.arbitrum.io/rpc

# Optional Service Configuration
CHAIN=unichain  # Chain to index (default: unichain)
PORT=3000       # Service port (default: 3000)
TYCHO_SERVICE_URL=http://localhost:3000  # Service URL for Python client
RUST_LOG=info   # Rust logging level

# Strategy Parameters
OUTPUT_FILE=strategy_state.csv
TARGET_LEVERAGE=3
LEVERAGE_BUFFER=0.5
ASSET=ETH
REBALANCE_SCHEDULE=600      # 10 minutes in seconds
POSITION_CHECK_SCHEDULE=60  # 1 minute in seconds
FUNDING_RATE_LOOKBACK_PERIOD=604800  # 7 days in seconds
MIN_BRIDGE_AMOUNT=1
MIN_SWAP_AMOUNT=1

# Gas Management Parameters
MIN_ETH_BALANCE=0.0025   # Trigger threshold for gas refill
TARGET_ETH_BALANCE=0.005 # Target ETH balance after refill